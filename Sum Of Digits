[Sum Of Digits](https://practice.geeksforgeeks.org/problems/sum-of-digits1742/0)



class Solution {

    sumOfDigits(N){
        let sum=0;let rem=0;
        //code here
        while(N>0){
            rem=N%10;
            sum+=rem;
            N=N/10;
        }
        return sum;
    }
}
